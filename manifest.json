{
  "manifest_version": 2,
  "name": "uWaterloo Schedule Exporter",
  "version": "1.9",
  "description": "Export your University of Waterloo class schedule directly from Quest.",
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://quest.pecs.uwaterloo.ca/*"],
      "all_frames": true,
      "js": [
        "jquery-2.2.3.min.js",
        "moment-with-locales.min.js",
        "main.js"
      ]
    }
  ]
}
